# KRAI Dashboard Frontend Environment Variables
# Copy this file to .env.local for local development
# Never commit .env.local or other environment files containing secrets

# API Configuration
# Use relative /api path to leverage Vite proxy during development
# For production, set to absolute backend URL (e.g., https://api.example.com)
VITE_API_BASE_URL=/api
VITE_API_TIMEOUT=30000

# Authentication Storage Keys
VITE_TOKEN_STORAGE_KEY=krai_auth_token
VITE_REFRESH_TOKEN_STORAGE_KEY=krai_refresh_token

# Feature Flags
VITE_ENABLE_DEVTOOLS=true
