{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "shadcn": "npx shadcn@latest",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "PWDEBUG=1 playwright test",
    "test:e2e:update-snapshots": "playwright test --update-snapshots",
    "test:e2e:auth": "playwright test auth.spec.ts",
    "test:e2e:crud": "playwright test documents-crud.spec.ts products-crud.spec.ts",
    "test:e2e:monitoring": "playwright test monitoring.spec.ts",
    "test:e2e:permissions": "playwright test permissions.spec.ts",
    "test:e2e:navigation": "playwright test navigation.spec.ts",
    "test:e2e:accessibility": "playwright test accessibility.spec.ts",
    "test:e2e:performance": "playwright test performance.spec.ts",
    "test:e2e:visual": "playwright test visual-regression.spec.ts",
    "test:e2e:error-handling": "playwright test error-handling.spec.ts",
    "test:e2e:integration": "playwright test integration-flows.spec.ts",
    "test:e2e:all-suites": "playwright test --grep=\"(Navigation|Accessibility|Performance|Visual|Error|Integration)\"",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:ci": "npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.1.6",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-table": "^8.20.5",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.469.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^7.1.3",
    "recharts": "2.15.4",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.3",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4.1.16",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.10.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^2.1.8",
    "axe-playwright": "^2.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.1.0",
    "msw": "^2.7.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.1.7",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^2.1.8"
  }
}
