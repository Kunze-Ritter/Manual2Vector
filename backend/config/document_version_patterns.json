{
  "document_version_patterns": {
    "description": "Manufacturer-specific document version patterns - Each manufacturer has their own versioning scheme",
    "manufacturer_specific": {
      "hp": {
        "name": "HP Document Versions",
        "description": "HP uses Edition-based versioning with dates",
        "patterns": [
          {
            "pattern": "edition\\s+(\\d+(?:\\.\\d+)?)\\s*,?\\s*(\\d+/\\d{4})",
            "example": "Edition 3, 5/2024",
            "priority": 1
          },
          {
            "pattern": "edition\\s+(\\d+(?:\\.\\d+)?)",
            "example": "Edition 4.0",
            "priority": 2
          }
        ]
      },
      "konica_minolta": {
        "name": "Konica Minolta Document Versions",
        "description": "Konica Minolta uses date-based versioning (YYYY/MM/DD)",
        "patterns": [
          {
            "pattern": "(\\d{4}/\\d{2}/\\d{2})",
            "example": "2024/12/25",
            "priority": 1
          },
          {
            "pattern": "(\\d{4}\\.\\d{2}\\.\\d{2})",
            "example": "2024.01.15",
            "priority": 2
          }
        ]
      },
      "lexmark": {
        "name": "Lexmark Document Versions",
        "description": "Lexmark uses month-year versioning",
        "patterns": [
          {
            "pattern": "([A-Z][a-z]+\\s+\\d{4})",
            "example": "November 2024",
            "priority": 1
          },
          {
            "pattern": "(\\d{2}/\\d{2}/\\d{4})",
            "example": "11/15/2024",
            "priority": 2
          }
        ]
      },
      "utax": {
        "name": "UTAX Document Versions",
        "description": "UTAX uses standard version numbering",
        "patterns": [
          {
            "pattern": "version\\s+(\\d+\\.\\d+(?:\\.\\d+)?)",
            "example": "Version 1.0",
            "priority": 1
          },
          {
            "pattern": "v\\s*(\\d+\\.\\d+)",
            "example": "v1.0",
            "priority": 2
          }
        ]
      },
      "triumph_adler": {
        "name": "Triumph Adler Document Versions",
        "description": "Triumph Adler uses version numbering and date formats",
        "patterns": [
          {
            "pattern": "version\\s+(\\d+\\.\\d+(?:\\.\\d+)?)",
            "example": "Version 1.0",
            "priority": 1
          },
          {
            "pattern": "(\\d{2}/\\d{4})",
            "example": "5/2024",
            "priority": 2
          }
        ]
      }
    },
    "generic_fallback": {
      "name": "Generic Document Versions",
      "description": "Fallback patterns for unknown manufacturers",
      "patterns": [
        {
          "pattern": "edition\\s+(\\d+(?:\\.\\d+)?)\\s*,?\\s*(\\d+/\\d{4})",
          "example": "Edition 3, 5/2024",
          "priority": 1
        },
        {
          "pattern": "(\\d{4}/\\d{2}/\\d{2})",
          "example": "2024/12/25",
          "priority": 2
        },
        {
          "pattern": "version\\s+(\\d+\\.\\d+)",
          "example": "Version 1.0",
          "priority": 3
        }
      ]
    },
    "extraction_notes": {
      "note_1": "Firmware versions (FW 4.2) are NOT extracted - only document versions",
      "note_2": "Only the FIRST match is returned - one document = one version",
      "note_3": "Manufacturer-specific patterns are prioritized over generic fallback",
      "note_4": "Search is case-insensitive",
      "note_5": "Patterns are tried in priority order (1 = highest priority)"
    }
  }
}
