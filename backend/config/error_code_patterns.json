  "extraction_rules": {
    "min_confidence": 0.75,
    "max_codes_per_page": 15,
    "context_window_chars": 200,
    "require_context_keywords": [
      "error", "code", "fault", "trouble", "malfunction", "failure", "alarm",
      "warning", "service", "maintenance", "troubleshooting", "diagnostic",
      "status", "message", "alert", "problem", "issue", "exception"
    ],
    "exclude_if_near": [
      "page", "figure", "table", "section", "model", "serial",
      "copyright", "trademark", "version", "revision"
    ],
    "error_indicators": [
      "error code", "error message", "fault code", "trouble code",
      "service code", "diagnostic code", "status code", "alarm code",
      "warning code", "malfunction code", "failure code"
    ]
  }
