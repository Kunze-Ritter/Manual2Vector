      "code\\s+([CJ]\\d{2}-\\d{2})",
      "([CJ]\\d{2}-\\d{2})\\s+error",
      "error\\s+code\\s+(\\d{2}\\.\\d{2})",
      "error\\s+(\\d{2}\\.\\d{2})",
      "code\\s+(\\d{2}\\.\\d{2})",
      "(\\d{2}\\.\\d{2})\\s+error",
