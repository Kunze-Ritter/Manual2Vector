{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Copier Error Code Patterns Configuration",
  "description": "Manufacturer-specific patterns for extracting error codes from documents and manuals",
  
  "hp": {
    "manufacturer_name": "HP",
    "description": "Hewlett Packard error code patterns",
    "format": "XX.XX.XX or XX.XXX.XX or XX.Xx.XX (x=wildcard)",
    "patterns": [
      "error\\s+code\\s+(\\d{2}\\.\\d{2,3}\\.\\d{2})",
      "error\\s+(\\d{2}\\.\\d{2,3}\\.\\d{2})",
      "code\\s+(\\d{2}\\.\\d{2,3}\\.\\d{2})",
      "(\\d{2}\\.\\d{2,3}\\.\\d{2})\\s+error",
      "\\b(\\d{2}\\.\\d{1,3}x\\.\\d{2})\\b",
      "\\b(\\d{2}\\.\\d{1,3}X\\.\\d{2})\\b",
      "\\b(\\d{2}\\.\\d{2,3}\\.\\d{2})\\b"
    ],
    "validation_regex": "^\\d{2}\\.\\d{1,3}[xX]?\\.\\d{2}$",
    "categories": {
      "paper_jam": "Paper handling and jam errors",
      "fuser": "Fuser assembly and temperature errors",
      "cooling": "Fan and cooling system errors",
      "motor": "Motor and mechanical errors",
      "laser": "Laser unit and beam errors",
      "scanner": "Scanner and imaging errors",
      "adf": "Automatic Document Feeder errors",
      "duplex": "Duplex printing errors",
      "toner": "Toner cartridge and supply errors",
      "drum": "Drum unit and imaging errors"
    }
  },
  
  "canon": {
    "manufacturer_name": "Canon",
    "description": "Canon error code patterns",
    "format": "E### or ####",
    "patterns": [
      "error\\s+code\\s+(E\\d{3,4})",
      "error\\s+(E\\d{3,4})",
      "code\\s+(E\\d{3,4})",
      "(E\\d{3,4})\\s+error",
      "error\\s+code\\s+(#\\d{3})",
      "error\\s+(#\\d{3})",
      "code\\s+(#\\d{3})",
      "(#\\d{3})\\s+error",
      "\\b(E\\d{3,4})\\b",
      "\\b(#\\d{3})\\b"
    ],
    "validation_regex": "^E\\d{3,4}$|^#\\d{3}$",
    "categories": {
      "fuser": "Fuser assembly and temperature errors",
      "scanner": "Scanner and imaging errors",
      "exposure": "Exposure lamp and LED errors",
      "paper_jam": "Paper handling and jam errors",
      "motor": "Motor and mechanical errors",
      "memory": "Memory and storage errors",
      "communication": "Communication and interface errors"
    }
  },
  
