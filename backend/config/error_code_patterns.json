      "error\\s+(E\\d{2}-\\d{2})",
      "code\\s+(E\\d{2}-\\d{2})",
      "(E\\d{2}-\\d{2})\\s+error",
      "\\b([CJ]\\d{4,5})\\b",
      "\\b([CJ]\\d{2}-\\d{2})\\b",
      "\\b(\\d{2}\\.\\d{2})\\b",
      "\\b(E\\d{2}-\\d{2})\\b"
    ],
    "validation_regex": "^[CJ]\\d{4,5}$|^[CJ]\\d{2}-\\d{2}$|^\\d{2}\\.\\d{2}$|^E\\d{2}-\\d{2}$",
    "categories": {
      "memory": "System memory and storage errors",
      "laser": "Laser unit and beam errors",
      "scanner": "Scanner and CIS errors",
      "paper_jam": "Paper handling and jam errors",
      "image_processing": "Image control and processing errors",
